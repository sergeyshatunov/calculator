<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="plus" class="by.shatunov.calculator.operations.Addition">
        <property name="a" value="0"/>
        <property name="b" value="0"/>
    </bean>

    <bean id="minus" class="by.shatunov.calculator.operations.Subtraction">
        <property name="a" value="0"/>
        <property name="b" value="0"/>
    </bean>

    <bean id="multiply" class="by.shatunov.calculator.operations.Multiplication">
        <property name="a" value="0"/>
        <property name="b" value="0"/>
    </bean>

    <bean id="divide" class="by.shatunov.calculator.operations.Division">
        <property name="a" value="0"/>
        <property name="b" value="1"/>
    </bean>

</beans>